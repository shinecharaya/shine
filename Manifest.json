{
    "manifest_version": 3,
    "name": "On Page SEO Extension",
    "version": "1.6",
    "description": "A comprehensive tool for analyzing website elements including links, meta information, headers (H1-H6), paragraphs, images, robots.txt, keywords, and color codes.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs",
      "downloads"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  